{
    "name": "that-dev-library",
    "description": "Library of standard dev dependencies and es lint config",
    "version": "0.3.1",
    "license": "UNLICENSED",
    "scripts": {
        "prepare": "npm config set registry https://registry.npmjs.org",
        "preinstall": "npm config set registry https://registry.npmjs.org"
    },
    "dependencies": {
        "@types/jest": "27.0.1",
        "@types/node": "16.6.1",
        "@typescript-eslint/eslint-plugin": "4.29.2",
        "@typescript-eslint/parser": "4.29.2",
        "depcheck": "1.4.2",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-import-resolver-typescript": "2.4.0",
        "eslint-plugin-i18next": "5.1.1",
        "eslint-plugin-import": "2.24.0",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-prettier": "3.4.0",
        "eslint-plugin-react": "7.24.0",
        "eslint-plugin-react-hooks": "4.2.0",
        "eslint-watch": "7.0.0",
        "jest": "27.0.6",
        "prettier": "2.3.2",
        "prettier-plugin-organize-imports": "2.3.3",
        "typescript": "4.3.5"
    },
    "jest": {
        "collectCoverageFrom": [
            "**/*.{js,jsx}",
            "!coverage/**/*",
            "!.eslintrc.js",
            "!babel.config.js",
            "!**/index.js"
        ]
    }
}
